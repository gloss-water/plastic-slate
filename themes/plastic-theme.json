  "tokenColors": [
    {
      "name": "Cloud",
      "scope": [
        "meta.brace",
        "punctuation.definition.array",
        "punctuation.definition.binding-pattern",
        "punctuation.definition.block",
        "punctuation.definition.dictionary",
        "punctuation.definition.string",
        "punctuation.definition.tag",
        "punctuation.section",
        "punctuation.separator",
        "punctuation.support",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#d0ccca"
      }
    },
    {
      "name": "Kangaroo",
      "scope": ["support.type.property-name", "support.variable", "variable"],
      "settings": {
        "foreground": "#c4c9c0"
      }
    },
    {
      "name": "Light Wisteria",
      "scope": [
        "markup.inline.raw",
        "storage.type",
        "support.constant",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#c5a7d9"
      }
    },
    {
      "name": "Mountain Meadow",
      "scope": ["constant", "keyword.other.unit"],
      "settings": {
        "foreground": "#15ab9c"
      }
    },
    {
      "name": "Shuttle Gray",
      "scope": ["comment"],
      "settings": {
        "foreground": "#5c6370"
      }
    },
    {
      "name": "Pastel Green",
      "scope": ["string", "markup.quote", "markup.bold", "markup.italic"],
      "settings": {
        "foreground": "#81d778"
      }
    },
    {
      "name": "Half Baked",
      "scope": [
        "entity.name.class",
        "entity.name.function",
        "entity.name.type",
        "entity.other.attribute-name",
        "entity.other.inherited-class",
        "markup.heading.setext",
        "support.function"
      ],
      "settings": {
        "foreground": "#7ab0d2"
      }
    },
    {
      "name": "Lilac Blush",
      "scope": ["entity.name.tag", "storage", "variable.language"],
      "settings": {
        "foreground": "#a481d3"
      }
    },
    {
      "name": "Light Orchid",
      "scope": [
        "entity.name.section",
        "keyword",
        "punctuation.definition.heading",
        "punctuation.definition.keyword"
      ],
      "settings": {
        "foreground": "#e2a8bf"
      }
    },
    {
      "name": "Bold",
      "scope": ["markup.bold.markdown", "punctuation.definition.bold.markdown"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Italic",
      "scope": [
        "comment",
        "markup.italic.markdown",
        "punctuation.definition.italic.markdown"
      ],
      "settings": {
        "fontStyle": "italic"
      }
    }
  ]
}
